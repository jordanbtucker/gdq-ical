{
  "name": "gdq-ical",
  "version": "0.0.0",
  "description": "Starting point for Node.js projects",
  "private": true,
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": "^10.17 || ^12 || ^14"
  },
  "scripts": {
    "coverage": "run-s -c tap tap-coverage",
    "eslint": "eslint --fix .",
    "lint": "run-s eslint prettier",
    "prettier": "prettier --loglevel warn --write **/*.{js,json,md,yml}",
    "preversion": "npm test",
    "start": "node lib",
    "tap": "tap test -R spec",
    "tap-coverage": "tap --coverage-report html",
    "test": "run-s lint tap"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "jsdom": "^16.4.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "sinon": "^9.0.2",
    "tap": "^14.10.7"
  }
}
